<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "PluginConfiguration.dtd">
<plugin>
	<name>UserIdentificationEnforceService</name>
	<classLocation/>
	<className>sk.fiit.rabbit.adaptiveproxy.plugins.services.user.UserIdentificationEnforceService</className>
	<types>
		<type>ResponseProcessingPlugin</type>
	</types>
	<parameters>
		<param name="notificationScript">
			<![CDATA[
				<script type='text/javascript'>
				  noapuidnotif = function($) {
				      $(document).ready(function() {
						  $.notifyBar({
						  	html: 'Please finish your proxy registration. <a href="http://peweproxy.fiit.stuba.sk/info/user_agent">Setup your User-Agent identifier</a>',
						  	delay: '20000',
						  	animationSpeed: 'normal',
						  	close: true
						  });
					  });
				  }(adaptiveProxyJQuery);
				</script>
			]]>
		</param>
	</parameters>
</plugin>